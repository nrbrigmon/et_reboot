{
	"name": "envision_reboot",
	"repository": {
		"type": "git",
		"url": "https://github.com/nrbrigmon/et_reboot.git"
	},
	"version": "1.0.0",
	"description": "moving envision tomorrow like land use planning apps to javascript",
	"main": "index.js",
	"engines": {
		"node": "8.1.0",
		"npm": "5.0.1"
	},
	"scripts": {
		"start": "node ./bin/www",
		"server": "nodemon ./bin/www",
		"config": "python ./bin/py/config_db_v2.py",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\" ",
		"postinstall": "NPM_CONFIG_PRODUCTION=true npm install --prefix client && npm run build --prefix client",
		"test": "npm run build --prefix client && nodemon ./bin/test",
		"qtest": "nodemon ./bin/test",
		"prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}"
	},
	"keywords": [
		"node",
		"react",
		"postgres"
	],
	"author": "nate brigmon",
	"license": "ISC",
	"dependencies": {
		"@turf/turf": "^5.1.6",
		"aws-sdk": "^2.263.1",
		"body-parser": "^1.18.2",
		"concurrently": "^3.5.1",
		"cookie-session": "^2.0.0-beta.3",
		"express": "^4.16.2",
		"geodesy": "^1.1.3",
		"json-stringify": "^1.0.0",
		"lodash": "^4.17.4",
		"moment": "^2.22.2",
		"morgan": "^1.9.0",
		"multer": "^1.3.0",
		"multer-s3": "^2.7.0",
		"nodemailer": "^4.6.7",
		"nodemon": "^1.12.1",
		"passport": "^0.4.0",
		"passport-google-oauth2": "^0.1.6",
		"passport-local": "^1.0.0",
		"pg": "^7.4.0",
		"shortid": "^2.2.8",
		"shpjs": "^3.4.2",
		"turf-clip": "0.0.1"
	},
	"devDependencies": {
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.10.0",
		"prettier": "1.13.7"
	}
}
