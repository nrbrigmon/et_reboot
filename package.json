{
	"name": "envision_reboot",
	"repository": {
		"type" : "git", 
		"url" : "https://github.com/nrbrigmon/et_reboot.git"
	},
	"version": "1.0.0",
	"description": "moving envision tomorrow like land use planning apps to javascript",
	"main": "index.js",
	"engines": {
		"node": "8.1.0",
		"npm": "5.0.1"
	},
	"scripts": {
		"start": "node ./bin/www",
		"server": "nodemon ./bin/www",
		"config": "python ./bin/config_db.py",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\" ",
		"postinstall":
		"NPM_CONFIG_PRODUCTION=true npm install --prefix client && npm run build --prefix client"

	},
	"keywords": ["node", "react", "postgres"],
	"author": "nate brigmon",
	"license": "ISC",
	"dependencies": {
		"@turf/turf": "^5.1.6",
		"body-parser": "^1.18.2",
		"concurrently": "^3.5.1",
		"cookie-session": "^2.0.0-beta.3",
		"express": "^4.16.2",
		"geodesy": "^1.1.3",
		"lodash": "^4.17.4",
		"morgan": "^1.9.0",
		"nodemon": "^1.12.1",
		"passport": "^0.4.0",
		"passport-google-oauth2": "^0.1.6",
		"pg": "^7.4.0",
		"shortid": "^2.2.8",
		"turf-clip": "0.0.1"
	}
}
